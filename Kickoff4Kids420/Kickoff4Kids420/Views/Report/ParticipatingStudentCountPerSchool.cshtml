@model IEnumerable<Kickoff4Kids420.ViewModels.ReportViewModel>
    @{
        ViewBag.Title = "ParticipatingStudentCountPerSchool";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }



    <div class="breadcrumbs margin-bottom-40">
        <div class="container">
            <h1 class="pull-left">Participating Students Per School Total</h1>
        </div>
    </div>
    <div class="container">
        <div class="panel panel-red">
            <div class="panel-heading">
                <h1 class="panel-title">School Active Students</h1>
            </div>
            <table class="table table-striped table-hover table-bordered">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.SchoolName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.State)
                    </th>
                    <th>
                            @Html.DisplayNameFor(model => model.Total_Participating_Students)
                        </th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.SchoolName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.State)
                        </td>
                        <td>
                                @Html.DisplayFor(modelItem => item.Total_Participating_Students)
                            </td>
                    </tr>
                }

            </table>
        </div>
    </div>
