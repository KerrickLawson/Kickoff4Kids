@{
    ViewBag.Title = "Home Page";  
}
<div class="slider-inner">
    <div id="da-slider" class="da-slider">
        <div class="da-slide">
            <h2><i> Kickoff 4 Kids</i> <br /> <i>House</i> <br /> <i>2013</i></h2>
            <div class="da-img"><img src="~/Images/k4k.jpg" height="300" width="600" alt="" /></div>
        </div>
        <div class="da-slide">
            <h2><i>Earn Points</i> <br /> <i>and</i> <br /> <i>win prizes today!</i></h2>

            <div class="da-img"><img src="~/Images/rewards.jpg" height="300" width="600" alt="image01" /></div>
        </div>
        <div class="da-slide">
            <h2><i>Get Involved</i> <br /> <i>And</i> <br /> <i>Make a Difference!</i></h2>

            <div class="da-img"><img src="/Images/student01.jpg" height="300" width="600" alt="image01" /></div>
        </div>
        <div class="da-arrows">
            <span class="da-arrows-prev"></span>
            <span class="da-arrows-next"></span>
        </div>
    </div>
</div>
<div class="purchase">
    <div class="container">
        <div class="row">
            <div class="col-md-9">
                <span>Are you interested in earning cool rewards while having fun? There's no time to waste!</span>
                @*<p>The Foundation’s mission is to give hope to kids, communities and to build unity amongst the children of Louisville, KY.</p>*@
            </div>
            <div class="col-md-3">
                <a href="/Account/Register" class="btn-buy hover-effect">Join the Competition!</a>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-4">
            <div class="service">
                <i class="icon-book service-icon"></i>
                <div class="desc">
                    <h4>Academics</h4>
                    <p>By excelling in the classroom students will have the opportunity to accumulate points and earn rewards based of their academic accomplishments!</p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="service">
                <i class="icon-trophy service-icon"></i>
                <div class="desc">
                    <h4>Extracurricular Activities</h4>
                    <p>Get involved with extracurricular activities at your school and earn points while you having fun!</p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="service">
                <i class="icon-group service-icon"></i>
                <div class="desc">
                    <h4>Community Service</h4>
                    <p>Contribute to your community and earn points at the same time! Volunteer your services and gain experiences that will last a lifetime!   </p>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="footer">
    <div class="container">
        <div class="row">
            <div class="col-md-4 md-margin-bottom-20">
                <div class="headline"><h2>Our Leaders</h2></div>
                <dl>
                    <dt>Chairman</dt>
                    <dd>Amobi Okoye</dd>
                    <dd>Augustine Okoye</dd>
                    <dd><br /></dd>
                    <dt>Pastor</dt>
                    <dd>Kenneth Tate</dd>
                    <dd><br /></dd>
                    <dt>Contributors</dt>
                    <dd>Adrian Garcia</dd>
                    <dd>Don Elder</dd>
                    <dd>Dr. Imo Aisiku</dd>
                    <dd>Judge Jimmy Thornton</dd>
                </dl>
            </div>
            <div class="col-md-4 md-margin-bottom-20">
                <div class="headline"><h2>About The Program</h2></div>
                <p>Kickoff 4 Kids is a program designed to get kids involved at school while having fun at the same time.
                    Participating students will be able to earn points by completing agreed upon activities. These points
                    can then be redeemed to purchase awards. </p><p>Through this system we hope to effect positive change within the community while making a 
                                                                     lasting impact on the lives of the youth of Louisville, KY. </p>
            </div>
                       
            <div class="col-md-4 md-margin-bottom-20">
                <div class="headline"><h2>Contact Us</h2></div>
                <address class="md-margin-bottom-40">
                    1832 Snake River Road <br />
                    Katy Tx 77449 <br />
                    Phone:  281-398-4201 <br />
                    Fax: 281-398-4211 <br />
                    Email: Info@amobiokoyefoundation.org
                </address>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    @Scripts.Render("~/bundles/index")
}


